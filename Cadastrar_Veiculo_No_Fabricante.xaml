<Window x:Class="ProejtoFinalPEOOFabricantedeVeiculos.Cadastrar_Veiculo_No_Fabricante"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProejtoFinalPEOOFabricantedeVeiculos"
        mc:Ignorable="d"
         Title="Cadastrar_Veiculo_No_Fabricante" Height="316" Width="423">
    <Grid Margin="0,0,10,6">
        <ListBox Name="listVeiculos" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,10,0,0" Width="370" Height="90" SelectionChanged="listVeiculo_SelectionChanged"/>
        <ListBox Name="listFabricantes" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,110,0,0" Width="370" Height="90"/>
        <Button Click="ListarClick" Content="Listar" HorizontalAlignment="Left" Margin="10,205,0,0" VerticalAlignment="Top"/>
        <Button x:Name="CadastroButton" Click="CadastroClick" Content="Cadastrar" HorizontalAlignment="Left" Margin="47,205,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>